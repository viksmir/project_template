{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Main Release (Windows AMD64)",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/out/bin/Windows/AMD64/Release/projTemplate_EXE.exe",
      "symbolSearchPath": "${workspaceFolder}/out/bin/Windows/AMD64/Release/",
      "console": "externalTerminal",
      "logging": {
        "moduleLoad": false,
        "trace": true
      },
      "visualizerFile": "${workspaceFolder}/my.natvis",
    },
    {
      "name": "Main Debug (Windows AMD64)",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/out/bin/Windows/AMD64/Debug/projTemplate_EXE.exe",
      "symbolSearchPath": "${workspaceFolder}/out/bin/Windows/AMD64/Debug/",
      "console": "externalTerminal",
      "logging": {
        "moduleLoad": false,
        "trace": true
      },
      "visualizerFile": "${workspaceFolder}/my.natvis",
    },
    {
      "name": "Main Unit Test Debug (Windows)",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/out/bin/Windows/AMD64/Debug/projTemplate_TEST.exe",
      "symbolSearchPath": "${workspaceFolder}/out/bin/Windows/AMD64/Debug/",
      "console": "externalTerminal",
      "stopAtEntry": false,
      "logging": {
        "moduleLoad": false,
        "exceptions": true,
        "trace": true,
      },
      "visualizerFile": "${workspaceFolder}/my.natvis",
    }
  ]
}